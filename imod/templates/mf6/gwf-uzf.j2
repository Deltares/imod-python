begin options
{%- if auxiliary is defined -%}  auxiliary {{auxiliary(naux)}}{%- endif -%}
{%- if auxmultname is defined -%}  auxmultname {{auxmultname}}{%- endif -%}
{%- if boundnames is defined -%}  boundnames{%- endif -%}
{%- if print_input is defined -%}  print_input{%- endif -%}
{%- if print_flows is defined -%}  print_flows{%- endif -%}
{%- if save_flows is defined -%}  save_flows{%- endif -%}
{%- if budget_filerecord is defined -%}  budget fileout {{budgetfile}}{%- endif -%}
{%- if ts_filerecord is defined -%}  ts6 filein {{ts6_filename}}{%- endif -%}
{%- if obs_filerecord is defined -%}  obs6 filein {{obs6_filename}}{%- endif -%}
{%- if mover is defined -%}  mover{%- endif -%}
{%- if simulate_et is defined -%}  simulate_et{%- endif -%}
{%- if linear_gwet is defined -%}  linear_gwet{%- endif -%}
{%- if square_gwet is defined -%}  square_gwet{%- endif -%}
{%- if simulate_gwseep is defined -%}  simulate_gwseep{%- endif -%}
{%- if unsat_etwc is defined -%}  unsat_etwc{%- endif -%}
{%- if unsat_etae is defined -%}  unsat_etae{%- endif -%}
end options

begin dimensions
  nuzfcells {{nuzfcells}}
  ntrailwaves {{ntrailwaves}}
  nwavesets {{nwavesets}}
end dimensions

begin packagedata
  open/close {{packagedata}}
end packagedata

{% for i, path in periods.items() %}begin period {{i}}
  open/close {{path}}
end period
{% endfor %}

